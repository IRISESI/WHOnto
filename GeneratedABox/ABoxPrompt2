@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix whc: <http://example.org/WorldHeritageConvention#> .

# Individuals representing Cultural Heritage
whc:TajMahal rdf:type whc:Monument ;
    rdfs:label "Taj Mahal" .

whc:HistoricCentreOfFlorence rdf:type whc:Site ;
    rdfs:label "Historic Centre of Florence" .

whc:HistoricCityOfToledo rdf:type whc:Site ;
    rdfs:label "Historic City of Toledo" .

# Individuals representing Natural Heritage
whc:GreatBarrierReef rdf:type whc:NaturalSite ;
    rdfs:label "Great Barrier Reef" .

whc:SerengetiNationalPark rdf:type whc:NaturalSite ;
    rdfs:label "Serengeti National Park" .

whc:GrandCanyonNationalPark rdf:type whc:NaturalSite ;
    rdfs:label "Grand Canyon National Park" .

# Individuals representing Mixed Cultural and Natural Heritage
whc:MachuPicchu rdf:type whc:MixedHeritage ;
    rdfs:label "Machu Picchu" .

whc:MountTaishan rdf:type whc:MixedHeritage ;
    rdfs:label "Mount Taishan" .

whc:MeteoraRock rdf:type whc:MixedHeritage ;
    rdfs:label "Meteora Rock" .
